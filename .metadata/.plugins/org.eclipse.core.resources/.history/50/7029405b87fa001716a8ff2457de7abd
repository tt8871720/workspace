package edu.fju;

public class Bubble {

	public static void main(String[] args) {
		int number1[] = {45,81,9,72,4,6};
		int number[] = {6,5,2,8,1,9,13,4,11,7};
for (int i = 0; i < number1.length; i++) {
	for (int j = 0; j <number1[i+1]; j++) {
		if(number1[j] > number1[i]){
			int tmp = number1[j];
			number1[j] = number1[i];
			number1[i] = number1[j];
			for (int k = 0; k < number1.length; k++) {
			System.out.print(number1[k] + " ");
		}
	}
	
}
	}
	}
}
